{
  "name": "express-ts",
  "version": "0.0.1",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "start": "bin/www",
    "ts-compile": "tsc",
    "ts-watch": "tsc -w",
    "nodemon": "nodemon --inspect bin/www",
    "clean": "rm -rf node_modules/ out/ dist/",
    "docker-build": "npm run build && docker build -t express-ts .",
    "docker-run": "docker run -p 3000:3000 express-ts",
    "lint": "eslint ."
  },
  "dependencies": {
    "@octokit/app": "^10.2.2",
    "@octokit/core": "^3.2.5",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.1.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "pug": "^3.0.2",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@redhat-actions/eslint-config": "^1.2.14",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/jquery": "^3.5.5",
    "@types/morgan": "^1.9.2",
    "@types/node": "14",
    "@types/node-fetch": "^2.5.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "nodemon": "^2.0.7",
    "typescript": "^4.2.2"
  }
}
