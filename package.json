{
  "name": "openshift-actions-connector",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/redhat-actions/openshift-actions-connector"
  },
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "webpack": "export WEBPACK_PACKAGE=webpack5; yarn ts-node ./node_modules/.bin/webpack-cli",
    "wds": "export WEBPACK_PACKAGE=webpack5; yarn ts-node ./node_modules/.bin/webpack-cli serve",
    "ts-node": "ts-node -O '{ \"module\": \"commonjs\" }'",
    "clean": "rm -rf build/",
    "dev:client": "wait-on -l tcp:3003 && react-scripts start",
    "dev:plugin:webpack": "yarn wds --config webpack.config.plugin.ts --mode=development",
    "dev:plugin": "concurrently -p name -c 'magenta,magenta,yellow' -n 'srv,srv,plugin' 'yarn dev:server:webpack' 'yarn dev:server:nodemon' 'yarn dev:plugin:webpack'",
    "dev:server": "concurrently -p name -c 'magenta,magenta' -n 'srv,srv' 'yarn dev:server:webpack' 'yarn dev:server:nodemon'",
    "dev:server:webpack": "yarn webpack --config webpack.config.server.ts --watch --mode=development",
    "dev:server:nodemon": "rm -f build/server.js && wait-on -l build/server.js && nodemon --inspect build/server.js",
    "dev": "concurrently -p name -c 'magenta,magenta,cyan' -n 'srv,srv,ui' 'yarn dev:server:webpack' 'yarn dev:server:nodemon' 'yarn dev:client'",
    "build:client": "react-scripts build",
    "build:server": "yarn webpack --config webpack.config.server.ts --mode=production",
    "build:plugin": "yarn webpack --config webpack.config.plugin.ts --mode=production",
    "build": "yarn clean && concurrently -p name -c 'yellow,magenta,cyan' -n 'ui,srv,plugin', 'yarn build:client' 'yarn build:server' 'yarn build:plugin'",
    "container-build": "buildah bud --layers -f containerize/Containerfile --build-arg VCS_REF=$(git rev-parse HEAD | cut -c -7) -t openshift-actions-connector:latest .",
    "container-build:dev": "buildah bud --layers -f containerize/dev/Containerfile.dev -t openshift-actions-connector-dev:latest",
    "container-run": "podman run --rm --name openshift-actions-connector -p 3000:3000 -p 3003:3003 openshift-actions-connector:latest",
    "container-run-d": "podman run -d --rm --name openshift-actions-connector -p 3000:3000 -p 3003:3003 openshift-actions-connector:latest",
    "container-tag": "export TAG=quay.io/tetchell/openshift-actions-connector:latest && buildah tag openshift-actions-connector:latest $TAG",
    "container-push": "export TAG=quay.io/tetchell/openshift-actions-connector:latest && buildah push $TAG",
    "container-e2e": "yarn container-build && yarn container-tag && yarn container-push",
    "helm-install": "export RELEASE_NAME=actions-connector && helm upgrade --install $RELEASE_NAME containerize/chart/openshift-actions-connector --set clusterApiServer=$(oc whoami --show-server)"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@kubernetes/client-node": "^0.14.0",
    "@octokit/auth-app": "^3.4.0",
    "@octokit/core": "^3.4.0",
    "@octokit/plugin-paginate-rest": "^2.13.3",
    "bootstrap": "^4.6.0",
    "classnames": "^2.3.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "http-proxy-middleware": "^1.1.0",
    "http-status-codes": "^2.1.4",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "node-fetch": "^2.6.1",
    "popper.js": "1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "sass": "^1.32.10",
    "serve": "^11.3.2",
    "session-file-store": "^1.5.0",
    "source-map-support": "^0.5.19",
    "tweetsodium": "^0.0.5",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@openshift-console/dynamic-plugin-sdk": "https://github.com/tetchel/console/raw/tim-test/frontend/packages/console-dynamic-plugin-sdk/%40openshift-console-dynamic-plugin-sdk-0.0.0-alpha9991.tar.gz",
    "@redhat-actions/eslint-config": "^1.3.1",
    "@types/bootstrap": "^5.0.12",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/node": "14",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/session-file-store": "^1.2.1",
    "@types/uid-safe": "^2.1.2",
    "@types/uuid": "^8.3.0",
    "@types/webpack-dev-server": "^3.11.4",
    "@types/webpack-node-externals": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "concurrently": "^6.0.0",
    "css-loader": "^5.2.4",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.22.0",
    "nodemon": "^2.0.7",
    "react-scripts": "^4.0.3",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "wait-on": "^5.3.0",
    "webpack": "~4.44",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^3.0.0",
    "webpack5": "npm:webpack@^5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "consolePlugin": {
    "name": "tims-plugin",
    "version": "0.1.0",
    "displayName": "Tim's amazing plugin",
    "description": "Tim's really amazing plugin",
    "exposedModules": {
      "welcome": "./src/client/pages/setup/welcome-page.tsx"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  }
}
