{
  "name": "openshift-actions-connector",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/redhat-actions/openshift-actions-connector"
  },
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -p name -c 'yellow,magenta,cyan' -n 'webpack,server,ui' 'yarn dev:server:webpack' 'yarn dev:server:nodemon' 'yarn dev:client'",
    "dev:client": "wait-on -l tcp:3003 && react-scripts start",
    "dev:server": "concurrently -p name -c 'yellow,magenta' -n 'webpack,server' 'yarn dev:server:webpack' 'yarn dev:server:nodemon'",
    "dev:server:webpack": "webpack --config webpack.config.server.js --watch",
    "dev:server:nodemon": "rm -f build/server.js && wait-on -l build/server.js && nodemon build/server.js",
    "build": "yarn build:client && yarn build:backend",
    "build:client": "react-scripts build",
    "build:server": "webpack --config webpack.config.server.js",
    "docker-build": "docker build -t openshift-actions-connector .",
    "docker-run": "docker run -p 3003:3003 --name openshift-actions-connector openshift-actions-connector:latest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@kubernetes/client-node": "^0.14.0",
    "@octokit/app": "^10.2.2",
    "@octokit/core": "^3.3.0",
    "bootstrap": "^4.6.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.1.4",
    "jquery": "^3.6.0",
    "log4js": "^6.3.0",
    "node-fetch": "^2.6.1",
    "popper.js": "1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@redhat-actions/eslint-config": "^1.2.14",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/log4js": "^2.3.5",
    "@types/node": "14",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.22.0",
    "nodemon": "^2.0.7",
    "react-scripts": "^4.0.3",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "wait-on": "^5.2.1",
    "webpack": "~4.44",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "proxy": "http://localhost:3003/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
