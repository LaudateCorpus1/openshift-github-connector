{
  "name": "openshift-actions-connector",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/redhat-actions/openshift-actions-connector"
  },
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "export NODE_ENV=dev; nodemon --inspect dist/server/index.js",
    "ts-compile": "tsc",
    "ts-watch": "tsc -w",
    "build": "npm run ts-compile",
    "clean": "rm -rf out/ dist/",
    "docker-build": "docker build -t openshift-actions-connector .",
    "docker-run": "docker run -p 3000:3000 --name openshift-actions-connector openshift-actions-connector:latest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.14.0",
    "@octokit/app": "^10.2.2",
    "@octokit/core": "^3.3.0",
    "bootstrap": "^4.6.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-react-views": "^0.11.0",
    "http-status-codes": "^2.1.4",
    "jquery": "^3.6.0",
    "log4js": "^6.3.0",
    "node-fetch": "^2.6.1",
    "popper.js": "^1.16.1",
    "react": "^16.14.0",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^16.14.0",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@redhat-actions/eslint-config": "^1.2.14",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/jquery": "^3.5.5",
    "@types/log4js": "^2.3.5",
    "@types/morgan": "^1.9.2",
    "@types/node": "14",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "nodemon": "^2.0.7",
    "typescript": "^4.2.3"
  }
}
